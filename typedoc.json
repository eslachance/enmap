{
  "entryPoints": ["./src/index.ts"],
  "out": "./docs/typedoc",
  "plugin": ["typedoc-plugin-markdown"],
  "name": "Enmap API Reference",
  "readme": "none",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module", 
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "gitRevision": "main",
  "githubPages": false,
  "cleanOutputDir": false,
  "sanitizeComments": true,
  "preserveAnchorCasing": false,
  "useCodeBlocks": true,
  "expandObjects": false,
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "enumMembersFormat": "table",
  "textContentMappings": {
    "title.indexPage": "API Reference",
    "title.memberPage": "{name}"
  }
}
